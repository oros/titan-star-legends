{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "./dist/",
    "paths": {
      "assets/*": [
        "client/assets/*"
      ],
      "components/*": [
        "client/components/*"
      ],
      "foundation/*": [
        "client/foundation/*"
      ],
      "styles/*": [
        "client/styles/*"
      ],
      "types/*": [
        "client/types/*"
      ],
    },
    "sourceMap": true,
    "target": "es6",
  }
}
